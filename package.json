{
  "name": "e-commerce",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "cosmos": "yarn workspace ui cosmos",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "web": "turbo run dev --filter=web"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@3.4.1"
}
